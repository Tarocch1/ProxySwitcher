{
  "manifest_version": 2,
  "name": "Proxy Switcher",
  "version": "1.0.0",
  "default_locale": "zh_CN",
  "description": "代理切换工具",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "proxy"
  ]
}
